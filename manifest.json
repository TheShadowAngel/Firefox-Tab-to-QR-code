{
	"manifest_version": 3,
	"name": "Tab to QR-code",
	"version": "3.0",
	"author": "Angel Lab",
	"homepage_url": "https://github.com/TheShadowAngel/Firefox-Tab-to-QR-code",
	"developer": {
		"name": "Angel Lab",
		"url": "https://github.com/TheShadowAngel"
	},
	"default_locale": "ru",
	"description": "__MSG_addonDescription__",
	"icons": {
		"64": "data/logo.png"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "alqrt@angel.lab",
			"strict_min_version": "109.0"
		}
	},
	"options_ui": {
		"page": "data/options.html",
		"browser_style": false
	},
	"action": {
		"browser_style": true,
		"default_icon": "data/logo.png",
		"default_title": "__MSG_addonTitle__",
		"default_popup": "data/index.html"
	},
	"background": {
		"scripts": [
			"data/background.js"
		]
	},
	"permissions": [
		"activeTab",
		"storage",
		"contextMenus",
		"menus"
	]
}